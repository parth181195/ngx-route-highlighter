{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "ngx",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": [
      false,
      140
    ]
  }
}
